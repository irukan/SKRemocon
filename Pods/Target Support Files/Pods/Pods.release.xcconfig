GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/IRKit" "${PODS_ROOT}/Headers/Public/ISHTTPOperation"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/IRKit" -isystem "${PODS_ROOT}/Headers/Public/ISHTTPOperation"
OTHER_LDFLAGS = -ObjC -l"Pods-IRKit" -l"Pods-ISHTTPOperation" -l"c++" -framework "AVFoundation" -framework "AudioToolbox" -framework "CoreGraphics" -framework "CoreTelephony" -framework "Foundation" -framework "MediaPlayer" -framework "QuartzCore" -framework "SystemConfiguration" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods